type Plugin {
    id: ID
    name: String
    appCode: String
    configurationDescriptors: [ConfigurationDescriptor]
}

type ConfigurationDescriptor{
    name: String
    displayName: String
    fieldType: FieldType
    description: String
    mandatory: Boolean
    allowedOptions: [String]
    validationExpr: String
}

enum FieldType{
    STRING, INT, LONG, BIG_INTEGER, BIG_DECIMAL, ENUM, BOOLEAN
}