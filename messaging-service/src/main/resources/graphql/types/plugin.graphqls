type Plugin {
    id: ID
    name: String
    appCode: String
    configurations: [Configuration]
}

type Configuration{
    name: String
    displayName: String
    fieldType: FieldType
    description: String
    mandatory: Boolean
    allowedOptions: [String]
    validationExpr: String
    value: String
}

enum FieldType{
    STRING, INT, LONG, BIG_INTEGER, BIG_DECIMAL, ENUM, BOOLEAN
}